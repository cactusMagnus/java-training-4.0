<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Homework" verbose="1" >
    <test name="SimpleTest">
        <classes>
            <class name="example.SimpleTest" />
        </classes>
    </test>
    <test name="TestClassOne">
        <groups>
            <run>
                <exclude name="Group 2"/>
            </run>
        </groups>
        <classes>
            <class name="example.TestClassOne" />
        </classes>
    </test>
    <test name="TestClassTwo">
        <parameter name="version" value="0.0.1"/>
        <parameter name="vendor" value="Some Company"/>
        <classes>
            <class name="example.TestClassTwo" />
        </classes>
    </test>
    <test name="TestClassThree">
        <classes>
            <class name="example.TestClassThree" />
        </classes>
    </test>
</suite>